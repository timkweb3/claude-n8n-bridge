{
  "mcpServers": {
    "n8n-api": {
      "command": "node",
      "args": ["/c/Users/Node/Desktop/Projects/n8n-mcp/src/index.js"],
      "env": {
        "N8N_BASE_URL": "https://n8n.actionforce.us",
        "N8N_API_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiIxZDA2NzRlNi0wNjY4LTQzZmQtYmQ5Zi0xOWFkMThjZDY4MGEiLCJpc3MiOiJuOG4iLCJhdWQiOiJwdWJsaWMtYXBpIiwiaWF0IjoxNzcwNzU5MDMxfQ.VVgaql-HWD0a0_6w9ag337de3BXrMis2vcqSrn4HYzQ"
      }
    },
    "n8n-knowledge": {
      "command": "npx",
      "args": ["n8n-mcp"],
      "env": {
        "MCP_MODE": "stdio",
        "LOG_LEVEL": "error",
        "DISABLE_CONSOLE_OUTPUT": "true"
      }
    },
    "google-workspace": {
      "command": "uvx",
      "args": ["workspace-mcp"],
      "env": {
        "GOOGLE_OAUTH_CLIENT_ID": "REPLACE_WITH_YOUR_CLIENT_ID",
        "GOOGLE_OAUTH_CLIENT_SECRET": "REPLACE_WITH_YOUR_CLIENT_SECRET",
        "OAUTHLIB_INSECURE_TRANSPORT": "1"
      }
    },
    "dbhub": {
      "command": "npx",
      "args": ["@bytebase/dbhub@latest", "--transport", "stdio", "--dsn", "REPLACE_WITH_YOUR_DSN"],
      "env": {
        "READONLY": "true"
      }
    },
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest", "--headless"]
    },
    "browser-tools": {
      "command": "npx",
      "args": ["@agentdeskai/browser-tools-mcp@latest"]
    },
    "slack": {
      "command": "npx",
      "args": ["-y", "slack-mcp-server@latest", "--transport", "stdio"],
      "env": {
        "SLACK_MCP_XOXC_TOKEN": "REPLACE_WITH_YOUR_XOXC_TOKEN",
        "SLACK_MCP_XOXD_TOKEN": "REPLACE_WITH_YOUR_XOXD_TOKEN"
      }
    },
    "rest-api": {
      "command": "npx",
      "args": ["-y", "dkmaker-mcp-rest-api"],
      "env": {
        "REST_BASE_URL": "https://n8n.actionforce.us"
      }
    }
  }
}
